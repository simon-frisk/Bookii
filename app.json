{
  "expo": {
    "name": "Bookii",
    "description": "Bookii as a platform for books - share books with your fiends and find awesome books to read!",
    "slug": "bookii",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "primaryColor": "#47f",
    "icon": "./assets/images/icon.png",
    "scheme": "bookiiapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#47f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bookii.bookiiapp",
      "buildNumber": "1.0.0",
      "requireFullScreen": false,
      "usesIcloudStorage": true
    },
    "githubUrl": "https://github.com/simon-frisk/Bookii"
  }
}
