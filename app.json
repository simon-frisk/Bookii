{
  "expo": {
    "name": "Bookii",
    "description": "Bookii as a platform for books - share books with your fiends and find awesome books to read!",
    "slug": "bookii",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "primaryColor": "#4477FF",
    "icon": "./assets/icon.png",
    "scheme": "bookiiapp",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4477FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bookii.bookiiapp",
      "requireFullScreen": false
    },
    "android": {
      "package": "com.bookii.bookiiapp"
    },
    "githubUrl": "https://github.com/simon-frisk/Bookii"
  }
}
